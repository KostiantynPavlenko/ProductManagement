  <Target Name="CopyApplicationToStaging">
    <Message Text="***** DEPLOYING APPLICATION COPY FROM $(AppLocation) TO STAGING DESTINATION, $(StagingRoot), FOR SMARTSOLVE SOLUTION DEPLOYMENT *****" />

    <PropertyGroup>
      <AppDestinationRoot>$(StagingRoot)\Application</AppDestinationRoot>
      <BinariesDestinationRoot>$(StagingRoot)\Binaries</BinariesDestinationRoot>
      <DbScriptsDestinationRoot>$(StagingRoot)\DbScripts</DbScriptsDestinationRoot>
      <DeltaBuildDestinationRoot>$(StagingRoot)\DeltaBuild</DeltaBuildDestinationRoot>
      <InitializationScriptsDestinationRoot>$(StagingRoot)\Initialization</InitializationScriptsDestinationRoot>
      <OracleScriptLocation>$(SolutionRoot)\DbScripts\Oracle\$(FileVersionMajor).$(FileVersionMinor).$(FileVersionService).$(FileVersionBuild)</OracleScriptLocation>
      <MsSqlScriptLocation>$(SolutionRoot)\DbScripts\MSSQL\$(FileVersionMajor).$(FileVersionMinor).$(FileVersionService).$(FileVersionBuild)</MsSqlScriptLocation>
    </PropertyGroup>

    <CreateItem Include="$(OutDir)\**\*.dll">
      <Output ItemName="BinaryFiles" TaskParameter="Include" />
    </CreateItem>

    <CreateItem Include="$(OracleScriptLocation)\**\*.*">
      <Output ItemName="OracleScriptsFiles" TaskParameter="Include" />
    </CreateItem>

    <CreateItem Include="$(MsSqlScriptLocation)\**\*.*">
      <Output ItemName="MsSqlScriptsFiles" TaskParameter="Include" />
    </CreateItem>

    <CreateItem Include="$(SmartSolveReleaseFilesLocation)\**\*.*">
      <Output ItemName="DeltaBuildFiles" TaskParameter="Include" />
    </CreateItem>

    <CreateItem Include="$(InitializationScriptsPathRoot)\**\*.*">
      <Output ItemName="InitializationScriptFiles" TaskParameter="Include" />
    </CreateItem>

    <!-- Application file -->
    <Message Text="***** Cleaning up staging directory: $(AppDestinationRoot) *****" Condition="'$(CleanStaging)'=='true'" />
    <RemoveDir Directories="$(AppDestinationRoot)" Condition="'$(CleanStaging)'=='true'" />

    <Message Text="***** Deploying to staging: $(AppDestinationRoot) *****" />
    <Exec Command="xcopy.exe &quot;$(AppLocation)&quot; &quot;$(AppDestinationRoot)&quot; /R /i /q /s /e /y " />

    <Message Text="***** Successfully deployed to staging directory: $(AppDestinationRoot) *****" />

    <!-- Binaries file -->
    <Message Text="***** Cleaning up staging directory for binaries: $(BinariesDestinationRoot) *****" Condition="'$(CleanStaging)'=='true'" />
    <RemoveDir Directories="$(BinariesDestinationRoot)" Condition="'$(CleanStaging)'=='true'" />

    <Message Text="***** Deploying to staging for binaries: $(BinariesDestinationRoot) *****" />
    <Copy SourceFiles="@(BinaryFiles)" DestinationFolder="$(BinariesDestinationRoot)\%(BinaryFiles.RecursiveDir)" ContinueOnError="$(OnErrorContinue)" SkipUnchangedFiles="true" />

    <Message Text="***** Successfully deployed to staging directory for binaries: $(BinariesDestinationRoot) *****" />

    <!-- DBScripts file -->
    <Message Text="***** Cleaning up staging directory for DBScripts: $(DbScriptsDestinationRoot) *****" Condition="'$(CleanStaging)'=='true'" />
    <RemoveDir Directories="$(DbScriptsDestinationRoot)" Condition="'$(CleanStaging)'=='true'" />

    <MakeDir Directories="$(DbScriptsDestinationRoot)" />

    <Message Text="***** Deploying to staging for binaries: $(DbScriptsDestinationRoot) *****" />
    <Copy SourceFiles="@(OracleScriptsFiles)" DestinationFolder="$(DbScriptsDestinationRoot)\Oracle\%(OracleScriptsFiles.RecursiveDir)" ContinueOnError="$(OnErrorContinue)" SkipUnchangedFiles="true" />
    <Copy SourceFiles="@(MsSqlScriptsFiles)" DestinationFolder="$(DbScriptsDestinationRoot)\MSSQL\%(MsSqlScriptsFiles.RecursiveDir)" ContinueOnError="$(OnErrorContinue)" SkipUnchangedFiles="true" />

    <Message Text="***** Successfully deployed to staging directory for DBScripts: $(DbScriptsDestinationRoot) *****" />

    <!-- Delta Build file -->
    <Message Text="***** Cleaning up staging directory for DeltaBuilds: $(DeltaBuildDestinationRoot) *****" Condition="'$(CleanStaging)'=='true'" />
    <RemoveDir Directories="$(DeltaBuildDestinationRoot)" Condition="'$(CleanStaging)'=='true'" />

    <Message Text="***** Deploying to staging for binaries: $(DeltaBuildDestinationRoot) *****" />
    <Copy SourceFiles="@(DeltaBuildFiles)" DestinationFolder="$(DeltaBuildDestinationRoot)\%(DeltaBuildFiles.RecursiveDir)" ContinueOnError="$(OnErrorContinue)" SkipUnchangedFiles="true" />

    <Message Text="***** Successfully deployed to staging directory for binaries: $(DeltaBuildDestinationRoot) *****" />

    <!-- Initialization scripts file -->
    <Message Text="***** Cleaning up staging directory for DeltaBuilds: $(InitializationScriptsDestinationRoot) *****" Condition="'$(CleanStaging)'=='true'" />
    <RemoveDir Directories="$(InitializationScriptsDestinationRoot)" Condition="'$(CleanStaging)'=='true'" />

    <Message Text="***** Deploying to staging for binaries: $(InitializationScriptsDestinationRoot) *****" />
    <Copy SourceFiles="@(InitializationScriptFiles)" DestinationFolder="$(InitializationScriptsDestinationRoot)\%(InitializationScriptFiles.RecursiveDir)" ContinueOnError="$(OnErrorContinue)" SkipUnchangedFiles="true" />

    <Message Text="***** Successfully deployed to staging directory for binaries: $(InitializationScriptsDestinationRoot) *****" />
  </Target>

